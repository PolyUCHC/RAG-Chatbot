# .env.example

# Local embedding model directory (download separately)
EMBED_MODEL_PATH=./bge-large-zh-v1.5

# Ollama model name
OLLAMA_MODEL=deepseek-r1:14b

# Chunking
CHUNK_SIZE=600
CHUNK_OVERLAP=100
TOP_K=5

# OCR (only if you install OCR deps)
OCR_LANG=chi_tra
OCR_DPI=200

# Optional: Poppler bin folder for pdf2image
# POPPLER_PATH=./poppler-25.12.0/Library/bin
